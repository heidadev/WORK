                                                <input type="text" class="form-control imput-sm" id="RQ" name="RQ" v-bind:disabled="textoButton == 'Guardar Cambios'" v-model="nRQ" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.RQ') }" disabled />
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row  mb-1">
                                        <div class="col-sm-12">
                                            <textarea class="form-control form-control-sm" rows="2" id="observacionSolicitud" placeholder="Observación (Indicar el detalle de la solicitud)" style="height:95%; resize:none; font-size: 9pt; padding-bottom: 90px;" v-model="observacionSolicitud" name="observacionSolicitud" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.observacionSolicitud') }"></textarea>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-7">
                                    <div class="panel">
                                        <header class="panel-heading panel-heading-blue">
                                            ...
                                        </header>
                                        <div class="panel-body">
                                            <div class="row mb-1">
                                                <label class="col-sm-2 form-control-label col-form-label-sm"> Área: </label>
                                                <div class="col-sm-10">
                                                    <select class="form-control select2 select2-hidden-accessible form-control-sm" tabindex="-1" aria-hidden="true" name="area" v-model="area" v-on:change="ObtenerCentroCosto" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.area') }">
                                                        <option value="" selected="selected">--- Seleccionar ---</option>
                                                        <option v-for="item in listaAreas" v-bind:value="item.codigo_area">{{item.descripcion_area}}</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="row mb-1">
                                                <label class="col-sm-2 form-control-label col-form-label-sm"> Sub Área: </label>
                                                <div class="col-sm-6">
                                                    <select class="form-control select2 select2-hidden-accessible form-control-sm" tabindex="-1" aria-hidden="true" name="centro_costo" v-model="centro_costo" v-on:change="Obtener_Cargo" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.centro_costo') }">
                                                        <option value="" selected="selected">--- Seleccionar ---</option>
                                                        <option v-for="item in listaCentroCostos" v-bind:value="item.codigo_centro_costo">{{item.descripcion_centro_costo}}</option>
                                                    </select>
                                                </div>

                                                <div class="col-sm-4">
                                                    <input id="ceco" name="ceco" v-model="ceco" type="text" class="form-control input-sm" placeholder="" disabled>
                                                </div>
                                            </div>

                                            <div class="row mb-1">
                                                <label class="col-sm-2 form-control-label col-form-label-sm"> Cargo: </label>
                                                <div class="col-sm-10">
                                                    <select class="form-control select2 select2-hidden-accessible form-control-sm" tabindex="-1" aria-hidden="true" name="cargo" v-model="cargo" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.cargo') }">
                                                        <option value="" selected="selected">--- Seleccionar ---</option>
                                                        <option v-for="item in listaCargos" v-bind:value="item.codigo_cargo">{{item.descripcion_cargo}}</option>
                                                    </select>
                                                </div>
                                            </div>

                                            <div class="form-group row mb-1" v-if="false">
                                                <label class="col-md-2 form-control-label col-form-label-sm">Sueldo: </label>
                                                <div class="col-sm-4">
                                                    <div class="input-group input-group-md">
                                                        <bage class="badge badge-primary">S/.</bage>
                                                        <input type="text" class="form-control rounded-0 form-control-sm" onpaste="return false;" onkeypress="return soloNumeros(event)" name="remuneracion" v-model="remuneracion" />
                                                    </div>
                                                </div>

                                                <label class="col-md-2 form-control-label col-form-label-sm">Variable: </label>
                                                <div class="col-sm-4">
                                                    <div class="input-group input-group-md">
                                                        <bage class="badge badge-primary">S/.</bage>
                                                        <input type="text" class="form-control rounded-0 form-control-sm" onpaste="return false;" onkeypress="return soloNumeros(event)" name="variable" v-model="variable" />
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="form-group row mb-1">
                                                <label class="col-md-2 form-control-label col-form-label-sm">Archivo: </label>
                                                <div class="col-sm-10" v-show="!mostrarFileInput">
                                                    <div class="input-group input-group-md">
                                                        <input type="text" class="form-control rounded-0 " v-model="nombreArchivoDet" disabled>
                                                        <span class="input-group-btn ">
                                                            <button type="submit" class="btn btn-primary rounded-0 " v-b-tooltip.hover title="Descargar" style="padding-bottom: 5px;padding-top: 5px;" v-on:click.prevent="descargarSustento"><i class="icon wb-download" aria-hidden="true"></i></button>
                                                        </span>
                                                    </div>
                                                </div>
                                                <div class="col-sm-10" v-show="mostrarFileInput">
                                                    <b-form-file class="form-control-file is-invalid" style="font-size:small;" v-model="file" ref="fileinput" name="file" v-validate="mostrarFileInput ? 'required' : ''" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.file') }" plain></b-form-file>
                                                    <p v-show="errors.has('formRegistroSolicitud.file')" class="text-danger" style="font-size:70%;">{{errors.first('formRegistroSolicitud.file') }}</p>
                                                </div>
                                            </div>

                                            <div class="form-group row mb-1">
                                                <div class="col-sm-2" v-show="textoButton == 'Guardar Cambios'">
                                                </div>
                                                <div class="col-sm-10" v-show="textoButton == 'Guardar Cambios'">
                                                    <div class="checkbox-custom checkbox-primary checkbox-sm">
                                                        <input type="checkbox" id="cambiarArchivo" v-model="mostrarFileInput" v-on:change="cambiarArchivoSustento">
                                                        <label for="cambiarArchivo" class="font-size-8">Cambiar Archivo de Sustento</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-12">
                                    <div class="panel">
                                        <header class="panel-heading panel-heading-blue">
                                            Datos del Contacto
                                        </header>
                                        <div class="panel-body">
                                            <div class="row mb-1">
                                                <label class="col-sm-1 form-control-label col-form-label-sm"> Nombre: </label>
                                                <div class="col-sm-4">
                                                    <input type="text" class="form-control rounded-0 form-control-sm" onpaste="return false;" name="nombreContacto" v-model="nombreContacto" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.nombreContacto') }" />
                                                </div>
                                                <label class="col-sm-1 form-control-label col-form-label-sm"> Cargo: </label>
                                                <div class="col-sm-3">
                                                    <input type="text" class="form-control rounded-0 form-control-sm" onpaste="return false;" name="cargoContacto" v-model="cargoContacto" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.cargoContacto') }" />
                                                </div>
                                                <label class="col-sm-1 form-control-label col-form-label-sm"> Anexo: </label>
                                                <div class="col-sm-2">
                                                    <input type="text" class="form-control rounded-0 form-control-sm" onpaste="return false;" onkeypress="return soloNumeros(event)" name="anexoContacto" v-model="anexoContacto" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </form>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-warning" data-dismiss="modal">Cerrar</button>

                        <button id="botonGA" type="button" class="btn btn-primary" v-on:click.prevent="guardar">
                            {{textoButton}}
                        </button>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="modal fade modal-fade-in-scale-up" id="modalDetalleSolicitud" aria-hidden="true" role="dialog" tabindex="-1" data-backdrop="static" data-keyboard="false">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="panel-heading panel-heading-gray custom-tab">
                        <h4 class="modal-title text-primary" id="exampleModalLabel"></h4>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>

                        <ul class="nav nav-tabs">
                            <li class="nav-item">
                                <a class="nav-link active" data-toggle="tab" href="#exampleLine1" aria-controls="exampleLine1" role="tab">
                                    <i class="fas fa-file-alt"></i>
                                    Detalle de Solicitud
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="tab" href="#exampleLine2" aria-controls="exampleLine2" role="tab">
                                    <i class="fas fa-list-ul"></i>
                                    Historial de Solicitud
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="modal-body form-horizontal">
                        <div class="tab-content">
                            <div class="tab-pane active" id="exampleLine1" role="tabpanel">
                                <div class="row mb-2">
                                    <div class="col-sm-5">
                                        <div class="form-group row mb-1">
                                            <label class="col-sm-4 form-control-label col-form-label-sm"> Motivo vacante: </label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control rounded-0 form-control-sm" v-model="motivoDet" v-bind:disabled="deshabilitar" />
                                            </div>
                                        </div>

                                        <div class="form-group row mb-1">
                                            <div class="col-sm-12">
                                                <input type="text" class="form-control rounded-0 form-control-sm" v-bind:disabled="deshabilitar" v-model="nombreEmpleadoDet" />
                                            </div>
                                        </div>

                                        <div class="form-group row mb-1">
                                            <label class="col-sm-4 form-control-label col-form-label-sm"> Fecha Ingreso: </label>
                                            <div class="col-sm-8">
                                                <input type="text" class="form-control rounded-0 form-control-sm" v-model="fechaIngresoDet" v-bind:disabled="deshabilitar" />
                                            </div>
                                        </div>

                                        <div class="form-group row mb-1">
                                            <label class="col-md-4 form-control-label col-form-label-sm">N° RQ: </label>
                                            <div class="col-sm-4">
                                                <div class="input-group input-group-md">
                                                    <input type="text" class="form-control rounded-0 form-control-sm" v-model="nRQDet" v-bind:disabled="deshabilitar" />
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row  mb-1">
                                            <div class="col-sm-12">
                                                <textarea class="form-control rounded-0 form-control-sm" style="resize:none; font-size: 9pt; padding-bottom: 65px;" rows="1" v-model="observacionDet" v-bind:disabled="deshabilitar"></textarea>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="col-sm-7">
                                        <div class="panel">
                                            <header class="panel-heading panel-heading-blue">
                                                ...
                                            </header>
                                            <div class="panel-body">
                                                <div class="row mb-1">
                                                    <label class="col-sm-2 form-control-label col-form-label-sm"> Área: </label>
                                                    <div class="col-sm-10">
                                                        <input type="text" class="form-control rounded-0 form-control-sm" v-model="areaDet" v-bind:disabled="deshabilitar" />
                                                    </div>
                                                </div>

                                                <div class="row mb-1">
                                                    <label class="col-sm-2 form-control-label col-form-label-sm"> Sub Área: </label>
                                                    <div class="col-sm-7">
                                                        <input type="text" class="form-control rounded-0 form-control-sm" v-model="centro_costoDet" v-bind:disabled="deshabilitar" />
                                                    </div>

                                                    <div class="col-sm-3">
                                                        <input type="text" class="form-control rounded-0 form-control-sm" v-model="cecoDet" v-bind:disabled="deshabilitar" />
                                                    </div>
                                                </div>

                                                <div class="row mb-1">
                                                    <label class="col-sm-2 form-control-label col-form-label-sm"> Cargo: </label>
                                                    <div class="col-sm-10">
                                                        <input type="text" class="form-control rounded-0 form-control-sm" v-model="cargoDet" v-bind:disabled="deshabilitar" />
                                                    </div>
                                                </div>

                                                <div class="form-group row mb-1" v-if="false">
                                                    <label class="col-md-2 form-control-label col-form-label-sm">Sueldo: </label>
                                                    <div class="col-sm-4">
                                                        <div class="input-group input-group-md">
                                                            <bage class="badge badge-primary">S/.</bage>
                                                            <input type="text" class="form-control rounded-0 form-control-sm" v-model="remuneracionDet" v-bind:disabled="deshabilitar" />
                                                        </div>
                                                    </div>

                                                    <label class="col-md-2 form-control-label col-form-label-sm">Variable: </label>
                                                    <div class="col-sm-4">
                                                        <div class="input-group input-group-md">
                                                            <bage class="badge badge-primary">S/.</bage>
                                                            <input type="text" class="form-control rounded-0 form-control-sm" v-model="variableDet" v-bind:disabled="deshabilitar" />
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="form-group row mb-1">
                                                    <label class="col-md-2 form-control-label col-form-label-sm">Archivo: </label>
                                                    <div class="col-sm-8">
