
@{
    ViewBag.Title = "Index";
}

<script src="~/Js/jquery.min.js"></script>
<link href="~/Librerias/bootstrap-datetimepicker-master/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
<script src="~/Librerias/bootstrap-datetimepicker-master/js/bootstrap-datetimepicker.min.js" charset="UTF-8"></script>
<script src="~/Librerias/bootstrap-datetimepicker-master/locales/bootstrap-datetimepicker.es.js"></script>


<script>

    function soloNumeros(e) {
        var key = window.Event ? e.which : e.keyCode
        return (key >= 48 && key <= 57)
    }

    function toggleFields() {
        var arr = ['3', '4', '5'];
        var value = $("#chosenmove1").val();

        if (jQuery.inArray(value, arr) > -1)
            $("#hideme").show();
        else
            $("#hideme").hide();
    }

</script>

<div id="appSPA">
    <div class="page-bar">
        <div class="page-title-breadcrumb">
            <div class="pull-left">
                <div class="page-title">Solicitud de Personal Administrativo</div>
            </div>

        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12">
            <div class="card card-box">
                <div class="card-head">
                    <header>Búsqueda de Solicitud</header>
                    <button id="panel-button6" class="mdl-button mdl-js-button mdl-button--icon pull-right" data-upgraded=",MaterialButton">
                        @*<i class="fas fa-search"></i>*@
                    </button>

                </div>
                <div class="card-body " id="bar-parent5">

                    <form class="form-horizontal" data-vv-scope="formSolicitudPersonal">
                        
                        <div class="form-group row">
                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                <label>Desde</label>
                                <input size="30" id="fechadesde" class="form-control" type="text" value="" v-model="fechaDesde" readonly="" v-bind:disabled="habilitaNRQ" v-validate="'required|date_format:DD/MM/YYYY'">
                            </div>

                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                <label>Hasta</label>
                                <input size="30" id="fechahasta" class="form-control" type="text" value="" v-model="fechaHasta" readonly="" v-bind:disabled="habilitaNRQ" v-validate="'required|date_format:DD/MM/YYYY'">
                            </div>

                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                <label> Área </label>
                                <select class="form-control select2 select2-hidden-accessible" tabindex="-1" aria-hidden="true" name="areaSearch" v-model="areaSearch" v-on:change="ObtenerCentroCostoSearch" v-bind:disabled="habilitaNRQ">
                                    <option value="" selected="selected">--- Seleccionar ---</option>
                                    <option v-for="item in listaAreasSearch" v-bind:value="item.codigo_area">{{item.descripcion_area}}</option>
                                </select>
                            </div>

                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                <label> Sub Área </label>
                                <select class="form-control select2 select2-hidden-accessible" tabindex="-1" aria-hidden="true" name="centro_costoSearch" v-model="centro_costoSearch" v-on:change="Obtener_CargoSearch" v-bind:disabled="habilitaNRQ">
                                    <option value="" selected="selected">--- Seleccionar ---</option>
                                    <option v-for="item in listaCentroCostosSearch" v-bind:value="item.codigo_centro_costo">{{item.descripcion_centro_costo}}</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group row">
                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                <label> Cargo </label>
                                <select class="form-control select2 select2-hidden-accessible" tabindex="-1" aria-hidden="true" name="cargoSearch" v-model="cargoSearch" v-bind:disabled="habilitaNRQ">
                                    <option value="" selected="selected">--- Seleccionar ---</option>
                                    <option v-for="item in listaCargosSearch" v-bind:value="item.codigo_cargo">{{item.descripcion_cargo}}</option>
                                </select>
                            </div>

                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                <label> Estado </label>
                                <select class="form-control select2 select2-hidden-accessible" tabindex="-1" aria-hidden="true" name="estado" v-model="estado" v-bind:disabled="habilitaNRQ">
                                    <option value="" selected="selected">--- Seleccionar ---</option>
                                    <option v-for="item in listaEstados" v-bind:value="item.codigo_multitabla" v-if="item.codigo_multitabla != 23 && item.codigo_multitabla != 28 && item.codigo_multitabla != 29 && item.codigo_multitabla != 31">{{item.descripcion_tabla}}</option>
                                </select>
                            </div>

                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                <div>
                                    <input type="checkbox" id="habilitaNRQ" v-model="habilitaNRQ" v-on:change="habilitarInputsSearch()">
                                    <label for="habilitaNRQ">
                                        Requerimiento
                                    </label>
                                </div>

                                <input type="text" class="form-control" id="horizontalFormText" name="nRQSearch" v-model="nRQSearch" v-bind:disabled="!habilitaNRQ" v-validate="habilitaNRQ ? 'required' : ''" v-bind:class="{'is-invalid': errors.has('formSolicitudPersonal.nRQSearch') }">
                                <p v-show="errors.has('formSolicitudPersonal.nRQSearch')" class="text-danger font-size-12">{{errors.first('formSolicitudPersonal.nRQSearch') }}</p>

                            </div>
                        </div>


                        <div class="form-group row">
                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 text-right">

                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 text-right">
                                <button type="submit" v-on:click.prevent="buscarSolicitud" class="btn btn-primary">Buscar Solicitud</button>
                            </div>

                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 text-left">
                                <button type="submit" class="btn btn-primary" v-on:click.prevent="nuevaSolicitud">Nueva Solicitud</button>
                            </div>
                        </div>

                        <div class="form-group row">
                            <div class="col-sm-12">
                                <div class="table-responsive">
                                    <template>
                                        <b-table striped hover small :fields="headTable" :items="listaSolicitud" :current-page="currentPage" :per-page="perPage" thead-class="tableHead">
                                            <template slot="accion" slot-scope="row">
                                                <button type="button" class="btn btn-warning btn-xs" v-b-tooltip.hover title="Mostrar Detalle" v-on:click.prevent="detalleSolicitud(row.item)">
                                                    <i class="fas fa-file"></i>
                                                </button>
                                                <button type="button" class="btn btn-primary btn-xs" v-b-tooltip.hover title="Editar" v-on:click.prevent="obtenerDatosSolicitud(row.item)" v-bind:disabled="row.item.edicion != true || row.item.estado == 'Aprobado'">
                                                    <i class="fas fa-edit"></i>
                                                </button>
                                            </template>
                                        </b-table>
                                        <b-pagination size="sm" :total-rows="listaSolicitud.length" :per-page="perPage" v-model="currentPage">
                                        </b-pagination>
                                    </template>
                                </div>
                            </div>
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="modal fade" id="SPAModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" data-backdrop="static" data-keyboard="false">
