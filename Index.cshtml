                                <div class="col-sm-5">
                                    <div class="form-group row mb-1">
                                        <label class="col-sm-4 form-control-label col-form-label-sm"> Motivo vacante: </label>
                                        <div class="col-sm-8">
                                            <select class="form-control select2 select2-hidden-accessible form-control-sm" tabindex="-1" aria-hidden="true" name="motivovacante" v-model="motivoVacante" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.motivovacante') }" v-on:change="habilitarNombrePersonalCesado()">
                                                <option value="" selected="selected">--- Seleccionar ---</option>
                                                <option v-for="item in listaMotivosVacante" v-bind:value="item.codigo_multitabla">{{item.descripcion_tabla}}</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="form-group row mb-1">
                                        <div class="col-sm-12">
                                            <input type="text" class="form-control rounded-0 form-control-sm" placeholder="Ingresar nombre del personal cesado" v-bind:disabled="habilitaNombrePersonalCesado" onpaste="return false;" name="nombreEmpleado" v-model="nombreEmpleado" v-validate="'required'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.nombreEmpleado') }" />
                                        </div>
                                    </div>

                                    <div class="form-group row mb-1">
                                        <label class="col-sm-4  form-control-label col-form-label-sm"> Fecha Ingreso: </label>
                                        <div class="col-sm-8">
                                            <input id="fechaingreso" class="form-control form-control-sm" name="fechaIngreso" v-model="fechaIngreso" type="text" readonly="" v-validate="'required|date_format:DD/MM/YYYY'" v-bind:class="{'is-invalid': errors.has('formRegistroSolicitud.fechaIngreso') }">
